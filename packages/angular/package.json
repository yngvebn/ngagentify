{
  "name": "@ng-annotate/angular",
  "version": "0.3.3",
  "schematics": "./schematics/collection.json",
  "description": "Angular library for ng-annotate-mcp â€” browser overlay for annotating components and routing instructions to an AI agent",
  "keywords": [
    "angular",
    "mcp",
    "ai",
    "devtools",
    "ng-annotate",
    "ng-add",
    "schematics"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yngvebn/ngagentify"
  },
  "files": [
    "dist",
    "schematics",
    "README.md"
  ],
  "main": "dist/fesm2022/ng-annotate-angular.mjs",
  "module": "dist/fesm2022/ng-annotate-angular.mjs",
  "types": "dist/types/ng-annotate-angular.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/ng-annotate-angular.d.ts",
      "default": "./dist/fesm2022/ng-annotate-angular.mjs"
    },
    "./package.json": { "default": "./package.json" }
  },
  "scripts": {
    "build": "ng-packagr -p ng-package.json && npm run build:schematics",
    "build:watch": "ng-packagr -p ng-package.json --watch",
    "build:schematics": "tsc -p schematics/tsconfig.json",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "peerDependencies": {
    "@angular/core": ">=21.0.0",
    "rxjs": ">=7.0.0"
  },
  "devDependencies": {
    "@angular-devkit/schematics": ">=17.0.0",
    "@angular/core": "^21.0.0",
    "ng-packagr": "^21.2.0",
    "rxjs": "^7.0.0",
    "typescript": "~5.9.0"
  }
}
