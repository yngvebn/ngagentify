{
  "name": "@ng-annotate/angular",
  "version": "0.5.16",
  "schematics": "./schematics/collection.json",
  "builders": "./builders.json",
  "description": "Angular library for ng-annotate-mcp â€” browser overlay for annotating components and routing instructions to an AI agent",
  "keywords": [
    "angular",
    "mcp",
    "ai",
    "devtools",
    "ng-annotate",
    "ng-add",
    "schematics"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yngvebn/ngagentify"
  },
  "files": [
    "dist",
    "schematics",
    "builders.json",
    "README.md"
  ],
  "main": "dist/fesm2022/ng-annotate-angular.mjs",
  "module": "dist/fesm2022/ng-annotate-angular.mjs",
  "types": "dist/types/ng-annotate-angular.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/ng-annotate-angular.d.ts",
      "default": "./dist/fesm2022/ng-annotate-angular.mjs"
    },
    "./package.json": {
      "default": "./package.json"
    }
  },
  "scripts": {
    "build": "ng-packagr -p ng-package.json && npm run build:schematics && npm run build:builders",
    "build:watch": "ng-packagr -p ng-package.json --watch",
    "build:schematics": "tsc -p schematics/tsconfig.json",
    "build:builders": "tsc -p builders/tsconfig.json && node -e \"require('fs').cpSync('src/builders/dev-server/schema.json','dist/builders/dev-server/schema.json')\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "peerDependencies": {
    "@angular-devkit/architect": ">=0.1700.0",
    "@angular/build": ">=21.0.0",
    "@angular/core": ">=21.0.0",
    "rxjs": ">=7.0.0"
  },
  "dependencies": {
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": ">=0.2100.0",
    "@angular-devkit/schematics": ">=17.0.0",
    "@angular/build": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@types/ws": "^8.18.1",
    "ng-packagr": "^21.2.0",
    "rxjs": "^7.0.0",
    "typescript": "~5.9.0",
    "vitest": "^4.0.18"
  }
}
